{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts",
    "../common/index.ts"
  ]
}
